<%# _form.html.erb %>

<%= form_with model: client do |form| %>
  <div>
    <%= form.label :first_name %><br>
    <%= form.text_field :first_name %>  
  </div>

  <div>
    <%= form.label :last_name %><br/>
    <%= form.text_field :last_name %>
  </div>

  <div>
    <%= form.label :email %><br/>
    <%= form.text_field :email %>
  </div>

  <div>
    <%= form.label :phone %><br/>
    <%= form.text_field :phone %>
  </div>

  <div>
    <%= form.submit  class: 'btn-lg-blue' %>
  </div>
<% end %>